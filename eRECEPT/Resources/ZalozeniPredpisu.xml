<v230:ZalozeniPredpisuDotaz xmlns:v230="http://www.sukl.cz/erp/201704" xmlns:v2301="http://www.sukl.cz/erp/201704">
  <v230:Doklad>
    <v2301:DatumVystaveni>${datumVystaveni}</v2301:DatumVystaveni>
    <v2301:PlatnostDo>${platnyDo}</v2301:PlatnostDo>
    <v2301:Rodina>${rodina}</v2301:Rodina>
    <v2301:Opakovani>${opakovani}</v2301:Opakovani>
    <v2301:Pacient>
      <v2301:Totoznost>
        <v2301:Jmeno>
          <v2301:Prijmeni>${prijmeni}</v2301:Prijmeni>
          <v2301:Jmena>${jmeno}</v2301:Jmena>
        </v2301:Jmeno>
        <v2301:DatumNarozeni>${datumNarozeni}</v2301:DatumNarozeni>
        <v2301:Adresa>
          <!--Optional:-->
          <v2301:NazevUlice>${ulice}</v2301:NazevUlice>
          <v2301:CisloPopisne>${cisloPopisne}</v2301:CisloPopisne>
          <v2301:NazevObce>${nazevObce}</v2301:NazevObce>
          <v2301:PSC>${psc}</v2301:PSC>
          
        </v2301:Adresa>
      </v2301:Totoznost>
      <v2301:CP>${cisloPojistence}</v2301:CP>
      <!--Optional:-->
      <v2301:ZP>${kodPojistovny}</v2301:ZP>
      <!--Optional:-->
      
      <!--Optional:-->
      <v2301:Telefon>${telefonPacienta}</v2301:Telefon>
      <!--Optional:-->
      <v2301:Email>${emailPacienta}</v2301:Email>
     
      <!--Optional:-->
       <v2301:Notifikace>${notifikace}</v2301:Notifikace>
      <!--Optional:-->
      <v2301:Veznice>${vezeni}</v2301:Veznice>
      <!--Optional:-->
      <v2301:Hmotnost>${hmotnost}</v2301:Hmotnost>
      <!--Optional:-->
      <v2301:Pohlavi>${pohlavi}</v2301:Pohlavi>
      <v2301:KontaktniAdresa>${kontaktniAdresa}</v2301:KontaktniAdresa>
    </v2301:Pacient>
    
    <v2301:Predepisujici>
      <v2301:Lekar>${idLekare}</v2301:Lekar>
      <v2301:ICP>${icp}</v2301:ICP>
      <v2301:PZS>${pzs}</v2301:PZS>
      <v2301:Telefon>${telefon}</v2301:Telefon>
      <v2301:Odbornost>${odbornost}</v2301:Odbornost>
    </v2301:Predepisujici>
    <v2301:Doporucujici>
      <v2301:Jmeno>
        <v2301:Prijmeni>${doporucujiciPrijmeni}</v2301:Prijmeni>
        <v2301:Jmena>${doporucujiciJmena}</v2301:Jmena>
     </v2301:Jmeno>
      <v2301:PZS>
        <v2301:Nazev>${doporucujiciNazevPZS}</v2301:Nazev>
        <v2301:ICZ>${doporucujiciIcz}</v2301:ICZ>
        <v2301:ICP>${doporucujiciIcp}</v2301:ICP>
        <v2301:IC>${doporucujiciIc}</v2301:IC>
        <v2301:DIC>${doporucujiciDic}</v2301:DIC>
        <v2301:Telefon>${doporucujiciTelefon}</v2301:Telefon>
      </v2301:PZS>
      <v2301:Odbornost>${doporucujiciOdbornost}</v2301:Odbornost>
    </v2301:Doporucujici>
    <!--1 to 2 repetitions:-->
   ${lek1}
   ${lek2}
    <!--Optional:-->
    <v2301:Pozn>${poznamka}</v2301:Pozn>
    <v2301:UpozornitLekare>${upozornitLekare}</v2301:UpozornitLekare>
    <v2301:Stav>${stav}</v2301:Stav>
    <v2301:DruhPojisteni>${druhPojisteni}</v2301:DruhPojisteni>
  </v230:Doklad>
  <v230:Zprava>
    <v2301:ID_Zpravy>${IdZpravy}</v2301:ID_Zpravy>
    <v2301:Verze>${Verze}</v2301:Verze>
    <v2301:Odeslano>${Odeslano}</v2301:Odeslano>
    <v2301:SW_Klienta>${Swklienta}</v2301:SW_Klienta>
  </v230:Zprava>
</v230:ZalozeniPredpisuDotaz>